{
  "sources": {
    "rand-input": {
      "key_dist": "org.apache.commons.math3.distribution.ZipfDistribution",
      "key_dist_params": {
        "num_keys": 10,
        "exponent": 1.2
      },
      "msg_dist": "org.apache.commons.math3.distribution.UniformIntegerDistribution",
      "msg_dist_params": {
        "lower": 20,
        "upper": 100
      },
      "rate_dist": "org.apache.commons.math3.distribution.UniformIntegerDistribution",
      "rate_dist_params": {
        "rate": 10
      }
    }
  },
  "transformations": {
    "filter1": {
      "operator": "filter",
      "input": "rand-input",
      "params": {
        "p": 0.5
      }
    },
    "split1": {
      "operator": "split",
      "input": "filter1",
      "params": {
        "n": 2
      },
      "outputs": ["split1__1", "split1__2"]
    },
    "filter2": {
      "operator": "filter",
      "input": "split1__1",
      "params": {
        "p": 0.9
      }
    },
    "modify1": {
      "operator": "modify",
      "input": "split1__2",
      "params": {
        "rate_ratio": 2.5
      }
    },
    "rand-output": {
      "operator": "merge",
      "inputs": ["filter2", "modify1"]
    }
  },
  "sinks": [
    "rand-output"
  ]
}
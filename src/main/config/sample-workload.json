{
  "sources": {
    "rand-input": {
      "key_dist": "org.apache.commons.math3.distribution.ZipfDistribution",
      "key_dist_params": {
        "num_keys": 10,
        "exponent": 1.2
      },
      "msg_dist": "org.apache.commons.math3.distribution.UniformIntegerDistribution",
      "msg_dist_params": {
        "lower": 20,
        "upper": 100
      },
      "rate_dist": "org.apache.commons.math3.distribution.UniformIntegerDistribution",
      "rate_dist_params": {
        "lower": 10,
        "upper": 10
      }
    }
  },
  "transformations": {
    "filter1": {
      "operator": "filter",
      "input": "rand-input",
      "params": {
        "p": 0.5
      }
    },
    "split1": {
      "operator": "split",
      "input": "filter1",
      "params": {
        "n": 2
      }
    },
    "rand-output": {
      "operator": "filter",
      "input": "split1__2",
      "params": {
        "p": 0.9
      }
    }
  }
}